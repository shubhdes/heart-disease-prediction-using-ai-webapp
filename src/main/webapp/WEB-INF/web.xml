<web-app xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
     http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">

	<display-name>heart-disease-prediction-using-ai-webapp</display-name>
	<absolute-ordering />
	<listener>
		<listener-class>com.hdp.controller.CommonListener</listener-class>
	</listener>
	<servlet>
		<servlet-name>CreateRegistrationFirstStepController</servlet-name>
		<servlet-class>com.hdp.controller.CreateRegistrationFirstStepController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CreateRegistrationSecondStepController</servlet-name>
		<servlet-class>com.hdp.controller.CreateRegistrationSecondStepController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FetchRegistration</servlet-name>
		<servlet-class>com.hdp.controller.FetchRegistration</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ForgotPasswordFirstStepController</servlet-name>
		<servlet-class>com.hdp.controller.ForgotPasswordFirstStepController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ForgotPasswordSecondStepController</servlet-name>
		<servlet-class>com.hdp.controller.ForgotPasswordSecondStepController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ForgotPasswordThirdStepController</servlet-name>
		<servlet-class>com.hdp.controller.ForgotPasswordThirdStepController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LoginController</servlet-name>
		<servlet-class>com.hdp.controller.LoginController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UpdatePasswordController</servlet-name>
		<servlet-class>com.hdp.controller.UpdatePasswordController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UpdateRegistrationController</servlet-name>
		<servlet-class>com.hdp.controller.UpdateRegistrationController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PredictionController</servlet-name>
		<servlet-class>com.hdp.controller.PredictionController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreateRegistrationFirstStepController</servlet-name>
		<url-pattern>/CreateRegistrationFirstStepController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CreateRegistrationSecondStepController</servlet-name>
		<url-pattern>/CreateRegistrationSecondStepController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FetchRegistration</servlet-name>
		<url-pattern>/FetchRegistration</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ForgotPasswordFirstStepController</servlet-name>
		<url-pattern>/ForgotPasswordFirstStepController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ForgotPasswordSecondStepController</servlet-name>
		<url-pattern>/ForgotPasswordSecondStepController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ForgotPasswordThirdStepController</servlet-name>
		<url-pattern>/ForgotPasswordThirdStepController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginController</servlet-name>
		<url-pattern>/LoginController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UpdatePasswordController</servlet-name>
		<url-pattern>/UpdatePasswordController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UpdateRegistrationController</servlet-name>
		<url-pattern>/UpdateRegistrationController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PredictionController</servlet-name>
		<url-pattern>/PredictionController</url-pattern>
	</servlet-mapping>
	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>
	<welcome-file-list>
		<welcome-file>jsp/home.jsp</welcome-file>
	</welcome-file-list>
</web-app>
